{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "new",
    "pluralName": "news",
    "displayName": "Noticias"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titulo": {
      "type": "string",
      "required": true
    },
    "Subtitulo": {
      "type": "string",
      "required": true
    },
    "Imagem": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "Corpo": {
      "type": "text",
      "required": true
    },
    "Autor": {
      "type": "string",
      "default": "Marcelo Lopes",
      "required": false
    }
  }
}
